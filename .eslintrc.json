{
    "env": {
      "browser": true,
      "es2021": true
    },
    "extends": "trybe-frontend",
    "parser": "babel-eslint",
    "parserOptions": {
      "ecmaFeatures": {
        "jsx": true
      },
      "sourceType": "module"
    },
    "ignorePatterns": [
      "**/*.js"
    ],
    "plugins": ["react", "sonarjs"],
    "rules": {
      "react/jsx-max-depth": "off"
    }
}
  
  